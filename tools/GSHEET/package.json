{
  "name": "gsheet-wizard",
  "version": "1.0.0",
  "description": "Google Sheets Wizard - Node.js + React Version",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"server,client\" --prefix-colors \"blue,green\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && node index.js",
    "dev:client": "cd client && npm run dev",
    "start": "npm run dev",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "build:client": "cd client && npx vite build",
    "setup": "node setup.js"
  },
  "keywords": [
    "google-sheets",
    "wizard",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {}
}

